model Staff {
    @@map("staff")

    id    Int     @id @default(autoincrement())
    firstname  String
    lastname  String
    email String
    phoneNumber String
    position StaffPosition @default(Other)
    status Boolean @default(false)
    imageUrl String?
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
    deletedAt DateTime?
    cars Car[]
}

enum StaffPosition {
    Professor
    Researcher
    Assistant
    Staff
    Student
    Other
}
