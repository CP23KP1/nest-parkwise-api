model User {
    @@map("user")

    id    Int     @id @default(autoincrement())
    email String  @unique 
    password String? 
    firstname  String @db.VarChar(255)
    lastname  String @db.VarChar(255)
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
    deletedAt DateTime?
}
