model Zone {
    @@map("zone")

    id    Int     @id @default(autoincrement())
    name  String
    description String?
    occupancy Int
    maximumCapacity Int
    address String?
    latitude Float? 
    longitude Float?
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
    deletedAt DateTime?
    devices Device[]
}
